# Destination Earth
constant marsClass = 'd1';

template_nofail default_step_units "grib2/localConcepts/destine/default_step_units.def";

# default stepUnits in minutes
#constant defaultStepUnits = 0 ;

# we don't want to have discipline=192 in Destination Earth
if ( discipline == 192 ) { meta error not_implemented(); }

alias parameter.paramId=paramId;
alias parameter.shortName=shortName;
alias parameter.units=units;
alias parameter.name=name;

alias mars.expver = marsExpver;
alias mars.class  = marsClass;
alias mars.stream = marsStream;
alias mars.type   = marsType;
alias mars.param = paramId;
alias mars.origin = centre;

if (stepType is "avg" || stepType is "accum") {
   alias mars.step = stepRange;
}
else {
   alias mars.step = step;
}
